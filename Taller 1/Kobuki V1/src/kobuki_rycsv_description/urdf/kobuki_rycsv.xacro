<?xml version='1.0'?>

<robot name="myrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">



  <xacro:include filename="$(find kobuki_rycsv_description)/urdf/kobuki_rycsv.gazebo" />
  <xacro:include filename="$(find kobuki_rycsv_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find kobuki_rycsv_description)/urdf/macros.xacro"/> 
  


  <link name='chassis'>
    <pose>0 0 0.0645 0 0 0</pose>
    <inertial>
      <mass value="2.4"/>
      <origin xyz="0.0 0 0.0645" rpy=" 0 0 0"/>
      <inertia
          ixx="0.0001" ixy="0" ixz="0"
          iyy="0.0001" iyz="0"
          izz="0.0001"
      />
    </inertial>

    <collision name='collision'>
      <geometry>
        <cylinder length=".089" radius=".177"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0.0645" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length=".089" radius=".177"/>
      </geometry>
    </visual>


    <collision name='caster_collision'>
      <origin rpy="0 0 0" xyz="0.0885 0 0.01"/>
      <geometry>
        <sphere radius="0.01"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster_visual'>
      <origin rpy="0 0 0" xyz="0.0885 0 0.01"/>
      <geometry>
        <sphere radius=".01"/>
      </geometry>
    </visual>
  </link>


  <link name="left_wheel">
    <!--origin xyz="0 -188 0.035" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0.035"/>
      <geometry>
        <cylinder length=".021" radius=".035"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0.035"/>
      <geometry>
        <cylinder length=".021" radius=".035"/>
      </geometry>
    </visual>
    <inertial>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0.035"/>
      <mass value="0.01"/>
      <cylinder_inertia m="0.01" r="0.035" h="0.021"/>
      <inertia
        ixx="0.001" ixy="0.0" ixz="0.0"
        iyy="0.001" iyz="0.0"
        izz="0.001"/>
    </inertial>
  </link>

  <link name="right_wheel">
    <!--origin xyz="0 188 0.035" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0.035"/>
      <geometry>
        <cylinder length=".021" radius=".035"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0.035"/>
      <geometry>
        <cylinder length=".021" radius=".035"/>
      </geometry>
    </visual>
    <inertial>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0.035"/>
      <mass value="0.01"/>
      <cylinder_inertia m="0.01" r="0.035" h="0.021"/>
      <inertia
        ixx="0.001" ixy="0.0" ixz="0.0"
        iyy="0.001" iyz="0.0"
        izz="0.001"/>
    </inertial>
  </link>


  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 -0.188 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 0.188 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>




</robot>



