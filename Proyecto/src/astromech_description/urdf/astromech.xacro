<?xml version='1.0'?>

<robot name="astromech" xmlns:xacro="http://www.ros.org/wiki/xacro">



  <xacro:include filename="$(find astromech_description)/urdf/astromech.gazebo" />
  <!--xacro:include filename="$(find astromech_description)/urdf/materials.xacro" /-->
  <xacro:include filename="$(find astromech_description)/urdf/macros.xacro"/> 
  


  <link name='chassis'>
    <pose>0 0 0.0081 0 0 0</pose>
    <inertial>
      <mass value="4"/>
      <origin xyz="0.0 0 0.0081" rpy=" 0 0 0"/>
      <inertia
          ixx="0.0001" ixy="0" ixz="0"
          iyy="0.0001" iyz="0"
          izz="0.0001"
      />
    </inertial>

    <collision name='collision'>
      <geometry>
        <box size="0.003 0.00687 0.0032"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0.0081" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.003 0.00687 0.0032"/>
      </geometry>
    </visual>


    <collision name='caster_collision'>
      <origin rpy="0 0 0" xyz="-0.007115 0 0.001085"/>
      <geometry>
        <sphere radius="0.001385"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster_visual'>
      <origin rpy="0 0 0" xyz="-0.007115 0 0.001085"/>
      <geometry>
        <sphere radius="0.001385"/>
      </geometry>
    </visual>
  </link>


  <link name="right_leg">
    <visual>
      <geometry>
        <box size="0.003 0.0007 0.01135"/>
      </geometry>
      <origin rpy="0 -0.95 0" xyz="0.005 0.0065 0.00224"/>
    </visual>
  </link>

  <link name="left_leg">
    <visual>
      <geometry>
        <box size="0.003 0.0007 0.01135"/>
      </geometry>
      <origin rpy="0 -0.95 0" xyz="0.005 -0.0065 0.00224"/>
    </visual>
  </link>

 


  <link name="left_wheel">
    <!--origin xyz="0 -188 0.035" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin rpy="0 1.5707 1.5707" xyz="0.009 -0.00257 0.000385"/>
      <geometry>
        <cylinder length=".001" radius=".001385"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0.009 -0.00257 0.000385"/>
      <geometry>
        <cylinder length=".001" radius=".001385"/>
      </geometry>
    </visual>
    <inertial>
      <origin rpy="0 1.5707 1.5707" xyz="0.009 -0.00257 0.000385"/>
      <mass value="0.001"/>
      <cylinder_inertia m="0.01" r="0.001385" h="0.001"/>
      <inertia
        ixx="0.001" ixy="0.0" ixz="0.0"
        iyy="0.001" iyz="0.0"
        izz="0.001"/>
    </inertial>
  </link>

  <link name="right_wheel">
    <!--origin xyz="0 188 0.035" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin rpy="0 1.5707 1.5707" xyz="0.009 0.00257 0.000385"/>
      <geometry>
        <cylinder length=".001" radius=".001385"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin rpy="0 1.5707 1.5707" xyz="0.009 0.00257 0.000385"/>
      <geometry>
        <cylinder length=".001" radius=".001385"/>
      </geometry>
    </visual>
    <inertial>
      <origin rpy="0 1.5707 1.5707" xyz="0.009 0.00257 0.000385"/>
      <mass value="0.001"/>
      <cylinder_inertia m="0.01" r="0.001385" h="0.001"/>
      <inertia
        ixx="0.001" ixy="0.0" ixz="0.0"
        iyy="0.001" iyz="0.0"
        izz="0.001"/>
    </inertial>
  </link>

  <joint name="base_to_left_leg" type="fixed">
    <parent link="chassis"/>
    <child link="left_leg"/>
    <origin xyz="0 0.0027 0.001385"/>
  </joint>
  
  <joint name="base_to_right_leg" type="fixed">
    <parent link="chassis"/>
    <child link="right_leg"/>
    <origin xyz="0 -0.0027 0.001385"/>
  </joint>


  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 -0.0027 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 0.0027 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="0.0"/>
  </joint>




</robot>
